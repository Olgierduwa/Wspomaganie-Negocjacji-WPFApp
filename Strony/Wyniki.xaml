<Window x:Class="negocjacja.Strony.Wyniki"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:negocjacja.Strony"
        mc:Ignorable="d"
        SizeToContent="WidthAndHeight"
        Title="Wyniki" Height="auto" Width="auto" MaxHeight="1000" MaxWidth="1800" FontSize="30">

    <DockPanel Background="DimGray" Height="auto" Width="auto">
        <TextBlock Background="Black" DockPanel.Dock="Top" Height="30"/>
        <TextBlock Background="Black" DockPanel.Dock="Bottom" Height="30"/>
        <Button Click="Zamknij" DockPanel.Dock="Bottom" Content="Zamknij"></Button>
        <DataGrid Name="KombinacjeLista" AutoGenerateColumns="True" Height="auto" Width="auto" IsReadOnly="true"></DataGrid>
    </DockPanel>
</Window>
